{
  "name": "@luxass/vscode-config",
  "version": "0.1.0",
  "description": "Typesafe Visual Studio Code configurations",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.28.2",
  "license": "MIT",
  "homepage": "https://github.com/luxass/vscode-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/vscode-config"
  },
  "bugs": {
    "url": "https://github.com/luxass/vscode-config/issues"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@types/vscode": ">=1.70.0"
  },
  "devDependencies": {
    "@luxass/eslint-config": "4.19.0",
    "@types/node": "24.10.13",
    "@types/vscode": "1.105.0",
    "eslint": "9.26.0",
    "eslint-plugin-format": "1.0.1",
    "tsup": "8.5.0",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  }
}
